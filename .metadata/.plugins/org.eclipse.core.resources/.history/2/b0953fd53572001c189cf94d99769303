package bar;

import java.io.File;
import java.io.FileWriter;
import java.io.IOException;

public class Kiiras {
	try {
	      FileWriter myWriter = new FileWriter("szamla.txt");
	      while() 
	      {
	    	  		myWriter.write("");
	      }
	      myWriter.close();
	    } catch (IOException e) {
	      System.out.println("Hiba");
	      e.printStackTrace();
	    }
	  
	}
